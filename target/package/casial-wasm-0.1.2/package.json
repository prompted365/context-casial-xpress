    "context",
    "coordination",
    "consciousness",
    "ai",
    "ubiquity-os",
    "mcp"
  ],
  "author": "Prompted LLC <hello@promptedllc.com>",
  "license": "Fair Use - See LICENSE.md",
  "homepage": "https://promptedllc.com",
  "scripts": {
